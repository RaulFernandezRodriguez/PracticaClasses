Class MainDate{
    private static void main(String arg[]){
        Date hoy = new Date(7,7,2022);
        Date ayer = new Date(6,7,2022);
    }
}

Class Date{
    int day, month, year;

    boolean isSameYear(Date another){
     if(this.year==another.getYear()){
     return true;
     } else return false;
    }
}