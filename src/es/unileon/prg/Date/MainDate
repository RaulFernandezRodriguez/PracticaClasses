package es.unileon.prg.Date;
Class MainDate{
    private static void main(String arg[]){
        Date hoy = new Date(day: 7,month: 7,year: 2022);
        Date ayer = new Date(day: 6,month: 7,year: 2022);
        hoy.isSameYear(ayer);
        hoy.isSame(ayer);
        hoy.getNumDaysSinceStartYear();
    }
}